# Uncomment the next line to define a global platform for your project
# platform :ios, '11.0'

target 'ARDashboard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ARDashboard
  pod 'TwitterKit'
  pod 'Kingfisher', :git => ‘https://github.com/onevcat/Kingfisher.git’, :branch => 'master'
  pod 'SnapKit'
  pod 'ReachabilitySwift'
  pod 'JPSVolumeButtonHandler'
  pod 'JGProgressHUD'
  pod 'SwiftyGif', :git => ‘https://github.com/kirualex/SwiftyGif’, :branch => 'swift4'
  pod 'Fabric'
  pod 'Crashlytics'

  target 'ARDashboardTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ARDashboardUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
